<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout13"
    android:layout_width="match_parent"

    android:layout_height="match_parent"
    android:layout_marginRight="0dp"
    android:background="@android:color/transparent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout14"
        android:layout_width="0dp"
        android:layout_height="200dp"
        android:background="@drawable/ic_r_popap_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/text_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="16dp"
            android:gravity="center"
            android:text="TextView"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TableLayout
            android:id="@+id/tabla_cuerpo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_name">

            <TableRow
                android:id="@+id/tableRow1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:id="@+id/id_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:padding="5dp"
                    android:text="ТОРГОВОЕ\nНАЙМЕННОВАНИЕ"
                    android:textColor="#000"
                    android:textSize="12sp"></TextView>

                <TextView
                    android:id="@+id/id_lf"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:padding="5dp"
                    android:text="ЛФ"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="12sp"></TextView>

                <TextView
                    android:id="@+id/id_doz"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:padding="5dp"
                    android:text="ДОЗА"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="12sp"></TextView>

                <TextView
                    android:id="@+id/id_count"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:padding="5dp"
                    android:text="КОЛИЧ."
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="12sp"></TextView>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:id="@+id/id_name2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cell_shape_2"
                    android:gravity="center"
                    android:padding="5dp"
                    android:text="ТОРГОВОЕ\nНАЙМЕННОВАНИЕ"
                    android:textColor="#000"
                    android:textSize="12sp"></TextView>

                <TextView
                    android:id="@+id/id_lf2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cell_shape_2"
                    android:gravity="center"
                    android:padding="5dp"
                    android:text="ЛФ"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="12sp"></TextView>

                <TextView
                    android:id="@+id/id_doz2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cell_shape_2"
                    android:gravity="center"
                    android:padding="5dp"
                    android:text="ДОЗА"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="12sp"></TextView>

                <TextView
                    android:id="@+id/id_count2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cell_shape_2"
                    android:gravity="center"
                    android:padding="5dp"
                    android:text="КОЛИЧ."
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="12sp"></TextView>

            </TableRow>

        </TableLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>